  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop --host localhost --port 3000 --open",
    "dev": "npm run develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "deploy:gh": "gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public,static . && prettier 'src/**/*.{js,jsx}' --list-different",
    "lint:md": "remark content/posts/",
    "lint": "npm run lint:js && npm run lint:md",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier 'src/**/*.{js,jsx}' --write"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
